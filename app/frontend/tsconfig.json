{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": [
        "src/*/index.js",
        "src/*/index.ts",
        "src/*/index.jsx",
        "src/*/index.tsx",
        "src/*/index.css",
        "src/*/index.css.ts"
      ],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@hooks/*": ["src/hooks/*"],
      "@pages/*": ["src/pages/*"],
      "@queries/*": ["src/queries/*"],
      "@services/*": ["src/services/*"],
      "@stores/*": ["src/stores/*"],
      "@styles/*": ["src/styles/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "extends": "@morak/tsconfig/frontend.json",
  "include": ["src", "src/types/custom.d.ts"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
