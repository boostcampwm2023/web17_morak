{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview --host 0.0.0.0",
    "prepare": "cd ../.. && husky install app/frontend/.husky",
    "lint-front": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --report-unused-disable-directives --quiet --fix",
      "git add"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.9",
    "@fullcalendar/daygrid": "^6.1.9",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.8.1",
    "@tanstack/react-query-devtools": "^5.8.1",
    "@vanilla-extract/css": "^1.14.0",
    "axios": "^1.6.1",
    "dayjs": "^1.11.10",
    "jotai": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@morak/eslint-config": "^1.0.0",
    "@morak/tsconfig": "^1.0.0",
    "@tanstack/eslint-plugin-query": "^5.6.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "vite": "^4.4.5"
  }
}
